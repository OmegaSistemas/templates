{
  "name": "cra-react-ts",
  "private": true,
  "description": "Descrição do projeto",
  "version": "0.0.1",
  "dependencies": {
    "@sentry/browser": "^7.13.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.21",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "axios": "^0.27.2",
    "axios-extensions": "^3.1.4",
    "classnames": "^2.3.2",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "i18next": "^21.9.2",
    "omega-react-ui": "latest",
    "omega-ui": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.6",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "sass": "^1.55.0",
    "typescript": "^4.8.3",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "auto-changelog": "^2.4.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "monkey-react-scripts": "^5.0.0-0",
    "prettier": "^2.7.1"
  },
  "scripts": {
    "start": "monkey-react-scripts start",
    "build": "monkey-react-scripts build",
    "test": "monkey-react-scripts test",
    "eject": "react-scripts eject",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --cache --fix --ignore-path ./.eslintignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,scss,md,json}\" --ignore-path ./.prettierignore",
    "lint": "yarn lint:format && yarn lint:fix",
    "prepare": "husky install",
    "changelog": "auto-changelog"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "auto-changelog": {
    "package": true,
    "commitLimit": false,
    "unreleased": true,
    "startingVersion": "v0.1.0",
    "template": "changelog-template.hbs",
    "commitUrl": "http://gitlab.omega/omega/<GITLAB_PROJECT>/commit/{id}",
    "compareUrl": "http://gitlab.omega/omega/<GITLAB_PROJECT>/compare/{from}...{to}",
    "tagPattern": "^((?!beta).)*$",
    "replaceText": {
      "[Ff]eature:": "",
      "[Ff]eat:": "",
      "[Ff]ix:": "",
      "[Bb]reak:": ""
    },
    "includeBranch": [
      "develop"
    ]
  }
}
